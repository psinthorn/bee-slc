{{!-- CC Information  - --}}
                     
                <div class="col s12 m12 l12" style="background-color: #fafafa; padding-top: 30px; padding-left: 20px;padding-right: 20px;">
                    <h4><b>Payment Information</b></h4>
                    <div class="hr-1px"></div>

                     <div class="row">

                            <div class="col s12 m6 l6 input-field">
                                  <select name="cccompany" id="cccompany" disabled>
                                           {{#select cccompany }}
                                           <option value="Visa Card" >Visa Card</option>
                                            <option value="Master Card" >Master Card</option>
                                            <option value="Amex" >Amex</option>
                                            <option value="JCB" >JCB</option>
                                            <option value="China Union Pay" >China Union Pay</option>
                                           {{/select }}
                            </select>
                                        <label for="cccompany">Card Company</label>
                            </div>
                            
                            <div class="col s12 m6 l6 input-field">
                                <input type="text"  name="ccno" value="{{ ccno }}" disabled>
                                <label for="ccno">Card No. (Ex. 1111-1111-1111-1111)</label>
                            </div>

                            
                    </div>

                     <div class="row">
                            <div class="col s12 m6 l6 input-field">
                                <input type="text"  name="ccexp" value="{{ ccexp }}" disabled>
                                <label for="ccexp">Expire Date ( dd/yy )</label>
                            </div>
                            
                            <div class="col s12 m6 l6 input-field">
                                <input type="text"  name="ccname" value="{{ ccname }}" disabled>
                                <label for="ccname">Name of Card Holder</label>
                            </div>

                            
                    </div>


                    


     
